<config>
	<executableName>${app.executable}</executableName>
	<mainClass>${app.mainclass}</mainClass>
	<os>ios</os>
	<arch>thumbv7</arch>
	
	<resources>
		<resource>
			<directory>resources</directory>
		</resource>
	</resources>

	<forceLinkClasses>
		<pattern>com.darwino.platform.web.PluginDarwinoWeb</pattern>
		<pattern>com.darwino.platform.web.tools.PluginDarwinoWebTools</pattern>
						
		<!-- URL Connection -->
		<pattern>com.android.okhttp.HttpHandler</pattern>
				
		<pattern>java.util.logging.ConsoleHandler</pattern>
		<pattern>java.util.logging.SimpleFormatter</pattern>
	</forceLinkClasses>
	
	
	<libs>
		<lib>libs/libJsonStoreUniversal.a</lib>
	</libs>

	<target>ios</target>
	<iosInfoPList>Info.plist.xml</iosInfoPList>
</config>